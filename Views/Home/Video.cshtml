
@{
    ViewBag.Title = "Video";
    Layout = "~/Views/Shared/_LayoutPublic.cshtml";
}

<object width="100%" height="600px">
    <param name="movie" value="@Request.QueryString["url"]">
    <param name="allowFullScreen" value="true">
    <param name="allowscriptaccess" value="always">
    <embed src="@Request.QueryString["url"]" type="application/x-shockwave-flash" allowscriptaccess="always" allowfullscreen="true" width="100%" height="600px">
</object>

